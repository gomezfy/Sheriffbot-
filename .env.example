# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_discord_client_id_here
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# Stripe Configuration (Optional - for payment processing)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Session Configuration
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
SESSION_SECRET=your_secure_random_64_char_hex_string_here

# Environment
NODE_ENV=development
PORT=5000

# Linked Roles Configuration (Optional - for Discord role verification)
LINKED_ROLES_PORT=3000
DISCORD_REDIRECT_URI=http://localhost:3000/discord/callback
# For production, use your actual domain:
# DISCORD_REDIRECT_URI=https://your-domain.com/discord/callback

# Bot Owner (Optional - for owner-only commands)
OWNER_ID=your_discord_user_id_here
